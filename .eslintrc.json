{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "eol-last": [
            "error",
            "never"
        ],
        "no-console": "off",
        "linebreak-style": ["error", "windows"]
    }
}
